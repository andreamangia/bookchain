# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /book/:id                          controllers.Application.bookId(id:Int)
GET     /user/:id                          controllers.Application.userid(id:Int)
GET     /bookmark/:id                      controllers.Application.bookmarkid(id:Int)
POST	/bookmark/create				   controllers.Application.bookmarkcreate	

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
